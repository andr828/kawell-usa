  margin-top: 80px;
  border-radius: 30px;
  overflow: hidden;
  box-shadow: 0 0 13px 3px #0003;
}

.farrier_main {
  display: flex;
}

.farrier--textarea {
  background: black;
  color: white;
  padding: 100px 10%;
  flex: 1;
  display: flex;
    flex-direction: column;
    align-items: flex-start;
    justify-content: center;
}

.farrier--textarea h2 {
  font-size: 36px;
  color: white;
  margin-bottom: 15px;
  color: #c45219;
}

.farrier_main--textarea p {
  font-size: 16px;
}

.farrier-image-container {
  position: relative;
}

.farrrier-image--side {
  height: 100%;
  object-fit: cover;
  min-width: 400px;
}

.farrier_main .farrrier-image--gradient {
  position: absolute;
  left: -1px;
  top: 0;
  height: 100%;
  width: 100%;
}

.btn-white {
  background: white;
  border: 2px solid white;
  border-radius: 20px !important;
  padding: 8px 25px;
  display: inline-block;
  color: black;
  font-size: 16px;
}

.btn-white:hover {
  background: black;
  color: white;
}

.farrier_quiz {
  padding: 80px;
  background: white;
}

.farrier_quiz h3 {
  margin-bottom: 30px;
}

.farrier_bottom {
  display: flex;
}

.farrier_bottom h3 {
  color: white;
  margin-bottom: 30px;
}

.farrier_bottom .farrrier-image--gradient {
  position: absolute;
  right: -1px;
  top: 0;
  height: 100%;
  width: 100%;
  transform: rotate(180deg);
}

@media screen and (max-width: 1440px) {
  .farriers-banner--right--main {
    padding: 30px;
  }
}

@media screen and (max-width: 1200px) {
  .farriers-banner {
    flex-direction: column;
  }

  .farrier_main {
    flex-direction: column;
  }

  .farrier_bottom {
    flex-direction: column;
  }

  .farrrier-image--gradient {
    display: none;
  }
}


@media screen and (max-width: 768px) {
  .farriers-page-main {
    padding: 15px;
  }

  .farriers-page-main p {
    font-size: 15px;
  }

  .farriers-banner--right--main h1 {
    font-size: 30px;
  }

  .farriers-banner--right--main p {
    font-size: 18px;
  }

  .farrier--textarea {
    padding: 30px 20px;
  }

  .farrier_quiz {
    padding: 20px;
  }

  .farrier {
    margin-top: 50px;
  }

}
