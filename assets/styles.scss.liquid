    i {
      display: inline-block;
      position: absolute;
      top: 0;
      right: 30px;
      width: 20px;
      height: 100%;
      transform: scale(.5);
      -webkit-transform: scale(.5);
      line-height: 50px;
      text-align: right;
      opacity: 0;
      transition: all .4s cubic-bezier(.175, .885, .32, .15);
