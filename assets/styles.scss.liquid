    span.collapse {
      display: block;
      position: absolute;
      top: 11px;
      right: 24px;
      height: auto;
      background-image: none;
      font-size: 0;
      text-indent: -9999px;
      cursor: pointer;
      z-index: 1;

      &::after {
        display: inline-block;
        width: 19px;
