// =============================================================================
// Screen Sizes
// =============================================================================
$theme-color: {{settings.theme_color}};
$screen-normal: "(max-width: 991px)";
$screen-medium: "(max-width: 767px)";
$screen-small: "(max-width: 575px)";
$theme-color: {{settings.theme_color}};
$color-white: #fff;
$color-black: #000;
$color-old-price: #a7a7a7;
$left: left;
$right: right;
$hot-bg: #62b959;
$sale-bg: #e27c7c;
$font-weight-semilight: 200;
$font-weight-light: 300;
$font-weight-normal: 400;
$font-weight-semibold: 600;
$font-weight-bold: 700;
$font-weight-extrabold: 800;
$enable-transitions: true !default;
$transition-base: all .2s ease-in-out !default;
$transition: all 0.25s ease 0s;

// spacement
$spacement-increment: 5px;
$spacement-xs: $spacement-increment;
$spacement-sm: $spacement-xs + $spacement-increment;
$spacement-md: $spacement-sm + $spacement-increment;
$spacement-lg: $spacement-md + $spacement-increment;
$spacement-xl: $spacement-lg + $spacement-increment;
$spacement-xlg: $spacement-xl + $spacement-increment; 
 
@mixin clearfix() {
  &::after {
    display: block;
    clear: both;
    content: "";
  }
}
@mixin transition($transition...) {
  @if $enable-transitions { 
    @if length($transition) == 0 {
      transition: $transition-base;
    } @else {
      transition: $transition;
    }
  }

  @media screen and (prefers-reduced-motion: reduce) {
    transition: none;
  }
}  
/*product*/
.product-image {
  display: block;
  position: relative;
  border: none;
  width: 100%;
  padding: 0;

  &:hover {
    z-index: 1;
  }

  .inner {
    display: block;
    overflow: hidden;
    position: relative;
  }

  img {
    display: inline-block;
    width: 100%;
    height: auto;
    transition: opacity 0.3s ease-in-out 0s;
    transform: translateZ(0);
  }
 

  .labels {
    line-height: 1;
    color: $color-white;
    font-weight: $font-weight-semibold;
    text-transform: uppercase;
    position: absolute;
    z-index: 2;
    top: .8em;
    font-size: 10px;
    left: .8em;

    .onhot,
    .onsale {
      padding: 5px 11px;
      margin-bottom: 5px;
    }

    .onhot {
      background: $hot-bg;
    }

    .onsale {
      background: $sale-bg;
    }

    .tooltip {
      font-weight: normal;
      text-transform: none;
      white-space: nowrap;
      z-index: 100;
    }
  }

  .stock {
    position: absolute;
    z-index: 1;
    color: #000;
    top: 0; bottom: 0; left: 0; right: 0;
    width: 10em;
    height: 3em; line-height: 3em;
    margin: auto;
    font-weight: $font-weight-semibold;
    text-transform: uppercase;
    text-align: center;
    //opacity: 0; 
  }

  &:hover {
    .stock {
      //bottom: 0;
      //opacity: 1;
    }
  }
}
.sale-product-daily-deal .daily-deal-title,
.sale-product-daily-deal .porto_countdown { font-family: 'Oswald'; text-transform: uppercase; }
.products {
  .sale-product-daily-deal {
    position: absolute;
    left: 10px;
    right: 10px;
    bottom: 10px;
    color: #fff;
    padding: 9px 0;
    text-align: center;
    &:before {
      content: '';
      position: absolute;
      left: 0;
      width: 100%;
      top: 0;
      height: 100%;
      background: $theme-color;
      opacity: 0.7;
    }
    >h5,>div, {
      position: relative;
      z-index: 1;
    }
    .offer-endtext {
      display: inline-block;
      color: #fff;
      font-size: 11px;
      font-weight: 400;
      margin-bottom: 0;
      margin-right: 1px;
    }
    .porto_countdown {
      float: none;
      display: inline-block;
      text-transform: uppercase;
      margin-bottom: 0;
      width: auto;
    }
    .porto_countdown-amount,.porto_countdown-period {
      font-size: 13px;
      font-weight: 500;
      padding: 0 1px;
    }
    .porto_countdown-section:last-child .porto_countdown-period {
      padding: 0;
    }
    &:after {
      content: '';
      display: table;
      clear: both;
    }
  }
} 
.products {
  &:not(.owl-carousel) {
    display: -webkit-flex; display: -ms-flexbox; display: flex;
    -webkit-flex-wrap: wrap; -ms-flex-wrap: wrap; flex-wrap: wrap;
  }
  .product-col {
    /*margin-bottom: 1.4286em;*/
    position: relative; 
    .product-content {
      padding-bottom: 1px;
    } 
    .product-image {
      margin-bottom: 16px;
      min-height: 90px;

      .img-effect {
        img {
          position: relative; 
          /*-webkit-backface-visibility: hidden;*/
          transform: translateZ(0);
        }

        .hover-image {
          position: absolute;
          left: 0; right: 0; top: 0; bottom: 0;
          margin: auto;
          opacity: 0;
        }
      }

      &:hover {
        .img-effect {
          img {
            opacity: 0;
          }
          .hover-image {
            opacity: 1;
          }
        }
      }
    }
    .product-loop-title {
      font-size: 1em;
      text-decoration: none;
    }
    h3 {
      font-size: 15px;
      font-weight: 400;
      line-height: 1.35;
      margin-bottom: 7.2px;
      -webkit-font-smoothing: auto;
    }
    .rating-wrap {
      margin: -3px 0 6px;
      height: 1px;
      position: relative;
      display: table;

      .rating-content {
        display: table-cell;
      } 
      .spr-badge-starrating {
        display: inline-block;
        margin: 0;
      }
      .spr-icon {top: 0;line-height: 1;display: inline-block;height: auto;}
      .spr-icon-star-empty:before,.spr-icon-star:before {
        content: '\e852';
        font-family: porto;
        font-size: 13px !important;
        margin: 0;
        line-height: 1;
      }
    }
    .description {
      margin-top: 1.5em;
    }
    .price {
      display: block;
      font-size: 18px;
      margin-bottom: 16px;
      line-height: 1;
      -webkit-font-smoothing: antialiased;
      font-weight: 600;
    }
    .add_to_cart_button { font-weight: 400; }
    .add-links {
      display: inline-block;
      position: relative;
      margin-bottom: 1.0714em;
      .tooltip-inner {
        white-space: nowrap;
      }
      .add_to_cart_read_more,
      .add_to_cart_button {
        transition: none;
      }
      .added_to_cart {
        display: none;
      }
      .yith-wcwl-add-to-wishlist > div {
        left: 0;
      }
      .link-compare {
        right: 0;
      }
      .yith-wcwl-add-to-wishlist > div,
      .quickview, .link-compare {
        position: absolute;
        top: 0;
        opacity: 0;
        visibility: hidden;
        z-index: 2;
      }
      div.quantity {
        margin-bottom: 5px;
      }
    }
    .links-on-image {
      height: 0;
      .add-links-wrap {
        display: none;
        height: 0;
      }
      .add-links {
        position: static;
      }
    }
    .variations {
      .label,
      .reset_variations {
        display: none !important;
      }  
      .filter-item-list {
        display: -webkit-flex; display: -ms-flexbox; display: flex;
        margin-top: -4px;
        margin-bottom: 5px;
        padding-bottom: 5px;
      }
    }
  }
  .variations { width: 100%; }
}
.products {
  .product:hover {
    .add-links {
      .yith-wcwl-add-to-wishlist > div,
      .quickview,.link-compare {
        opacity: .85;
        visibility: visible;
      }
    }
    .links-on-image .add-links-wrap {
      display: block;
    }
    .product-image {
      .viewcart.added {
        display: block;
        opacity: 1;
      }
      box-shadow: 0 25px 35px -5px rgba(0,0,0,0.1);
    }
  }
  .product {
    .product-inner {width: 100%;}
    .add-links {
      .quickview:hover {
        opacity: 1;
      }
    }
  }
  .product-default:hover {
    .add-links {
      .yith-wcwl-add-to-wishlist > div {
        left: -40px;
      }
      .link-compare {
        right: -40px;
      }
      &.no-effect {
        .add_to_cart_read_more,
        .add_to_cart_button {
          text-indent: 0; width: auto; padding: 0 0.7143em;
          &:before {
            position: static;
            font-size: 1em;
            margin-right: 0.5714em;
          }
        }
      }
      .add_to_cart_button,
      .add_to_cart_read_more {
        color: #fff;
      }
    }
  }
}
