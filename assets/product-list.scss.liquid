// shopify products widget
ul.product_list_widget,
.widget ul.product_list_widget {
  list-style: none outside;
  padding: 0;
  margin: 0;
  border-width: 0 !important;
  li {
    border-width: 0;
    position: relative;
    padding: .5714em 0 .5714em 104px;
    margin: 0;
    list-style: none;
    @include clearfix;

    &:first-child {
      padding-top: 0;
    }

    &:last-child {
      padding-bottom: 0;
    }

    .product-image {
      float: $left;
      width: 84px;
      padding: 0;
      margin-#{$left}: -104px; 
      img {
        width: 100%;
        height: auto;
      } 
      .img-effect {
        img {
          position: relative;
          opacity: 1;
        }

        .hover-image {
          position: absolute;
          left: 0; right: 0; top: 0; bottom: 0;
          margin: auto;
          opacity: 0;
        }
      }

      &:hover {
        .img-effect {
          img {
            opacity: 0;
          }
          .hover-image {
            opacity: 1;
          }
        }
      }
    } 
    .product-details {
      position: relative;
      padding-top: 3px;
      a {
        display: block;
        text-decoration: none;
        font-size: 13px;
        letter-spacing: -0.005em;
        line-height: 1.8;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        margin-bottom: 4px;
        color:#5b5b5f;
      }
      .amount {
        font-size: 1.1538em;
        font-weight: $font-weight-semibold;
        line-height: 1;
        vertical-align: middle; 
        color: #465157; 
        .currency,
        .decimal {
          font-size: .75em;
          font-weight: $font-weight-normal;
        }
        .currency {
          .decimal {
            font-size: 1em;
          }
        }
      }
      ins {
        text-decoration: none;
        vertical-align: baseline;
      }
      del,
      .from {
        color: $color-old-price;
        font-size: .8em;
        margin-#{$right}: 3px;
        vertical-align: baseline;
        .amount {
          color: $color-old-price;
        }
      }
    }
    dl {
      margin: 0;
      padding-#{$left}: 1em;
      border-#{$left}: 2px solid rgba(0, 0, 0, 0.1);
      @include clearfix;

      dt, dd {
        display: inline-block;
        float: $left;
        margin-bottom: 1em;
      }
      dt {
        font-weight: $font-weight-bold;
        padding: 0 0 .25em 0;
        margin: 0 4px 0 0;
        clear: $left;
      }
      dd {
        padding: 0 0 .25em 0;
        p:last-child {
          margin-bottom:0;
        }
      }
    }
    .star-rating { 
      margin: 3px 0 5px;
      .spr-badge {
        display: inline-block;
        .spr-badge-caption {display: none;}
      }
      .spr-icon {top: 0;line-height: 1;display: inline-block;height: auto;width: 14px;}
      .spr-icon-star-empty:before,.spr-icon-star:before {
        content: '\e852';
        font-family: porto;
        font-size: 13px !important;
        margin: 0;
        line-height: 1;
      }
    } 
  }
}
@media(max-width: 992px){
.porto-products.widget-products {margin-bottom: 35px;}
