<div data-section-id="{{ section.id }}" data-section-type="infomation-section">
  <div class="vc_custom_{{section.id}}"  style="margin-top: {{section.settings.margin_top | remove: 'px' | append: 'px' }};margin-bottom: {{section.settings.margin_bottom | remove: 'px' | append: 'px' }};padding: {{section.settings.padding}};">
    <div class="container"> 
        <div class="wpb_wrapper">
          {%- if section.blocks.size > 0 -%} 
            <div class="row vc_custom_{{section.id}} collection-spacing-{{section.settings.space_item}}">
              {%- for block in section.blocks -%} 
              <div class="item col-xl-{{block.settings.column_width_desktop}} col-md-{{block.settings.column_width_tablet}} col-sm-{{block.settings.column_width_tablet}} col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} hidden-xs{% endif%}{%if block.settings.background_position != 'default' %} porto-bg-{{block.settings.background_position | replace: '_', ' ' }}{% endif %} text-{{block.settings.column_text_alignment}}">
                <div class="vc_column-inner vc_box_custom_{{ block.id }}7">
                  <div class="wpb_wrapper"> 
                    <div class="info-box-wrapper {{block.settings.extra_class}}">
                      <div id="infobox-{{block.id}}" class="porto-info-box with-btn btn-position-{{block.settings.button_position}}  text-{{block.settings.text_alignment}} icon-alignment-{{block.settings.image_alignment}} box-{{block.settings.box_style}} color-scheme-{{block.settings.color_scheme}}{% if block.settings.box_style == 'bg-hover'%} hover-color-scheme-{{block.settings.color_scheme_hover}}{% endif %} porto-bg-none vc_custom_{{block.id}}5">
                        <div class="box-icon-wrapper with-{{block.settings.icon_type}}{% if block.settings.icon_type == 'text' %} text-size-{{block.settings.icon_text_size}}{% endif %} icon-style-{{block.settings.icon_style}}">
                          <div class="info-box-icon"> 
                            {%- if block.settings.icon_type == 'text' -%}
                              {{block.settings.icon_text}}
                            {%- endif -%}
                            {%- if block.settings.icon_type == 'icon' -%}
                              {%- assign imw = block.settings.icon_image_size | append: 'x'-%}
                              {%- if block.settings.icon_image != blank -%}
                              <img src="{{block.settings.icon_image | img_url: imw, crop: 'center'}}" /> 
                              {%- endif -%}
                            {%- endif -%}
                          </div> 
                        </div>
                        <div class="info-box-content"> 
                          {%- if block.settings.title != '' -%}
                          <h3 class="info-box-title" style="color: {{block.settings.title_color}};font-size: {{block.settings.title_size | remove: 'px' | append: 'px'}};font-weight: {{block.settings.title_font_weight}};line-height: {{block.settings.title_line_height | remove: 'px' | append: 'px'}};">
                            {%- if settings.language_enable -%}
                            <span class="lang1">{{ block.settings.title | split: '|' | first }}</span>
                            <span class="lang2">{{ block.settings.title | split: '|' | last }}</span>
                            {%- else -%}
                            {{ block.settings.title | split: '|' | first }}
                            {%- endif -%}
                          </h3>
                          {%- endif -%}
                          {%- if block.settings.sub_title != '' -%}
                          <p style="color:{{block.settings.subtitle_color}};font-size:{{block.settings.subtitle_size | remove: 'px'| append: 'px'}};font-weight:{{block.settings.subtitle_font_weight}};line-height:{{block.settings.subtitle_line_height | remove: 'px' | append: 'px'}};">
                            {%- if settings.language_enable -%}
                            <span class="lang1">{{ block.settings.sub_title | split: '|' | first }}</span>
                            <span class="lang2">{{ block.settings.sub_title | split: '|' | last }}</span>
                            {%- else -%}
                            {{ block.settings.sub_title | split: '|' | first }}
                            {%- endif -%}
                          </p>
                          {%- endif -%}
                          {%- if block.settings.content != '' -%}
                          <div class="info-box-inner">
                            {%- if settings.language_enable -%}
                            <div class="lang1">{{ block.settings.content | split: '[lang2]' | first }}</div>
                            <div class="lang2">{{ block.settings.content | split: '[lang2]' | last }}</div>
                            {%- else -%}
                            {{ block.settings.content | split: '[lang2]' | first }}
                            {%- endif -%}
                          </div>
                          {%- endif -%}
                          {%- if block.settings.button_title != '' -%}
                          <div class="info-btn-wrapper">
                            <div class="porto-button-wrapper text-{{block.settings.column_text_alignment}}">
                              <a href="{{block.settings.button_link}}" class="btn-button" style="background-color: {{block.settings.button_bg_color}};color:{{block.settings.button_text_color}};">
                                {%- if settings.language_enable -%}
                                <span class="lang1" style="color:inherit;">{{ block.settings.button_title | split: '|' | first }}</span>
                                <span class="lang2" style="color:inherit;">{{ block.settings.button_title | split: '|' | last }}</span>
                                {%- else -%}
                                {{ block.settings.button_title | split: '|' | first }}
                                {%- endif -%}
                              </a>
                            </div>
                          </div>
                          {%- endif -%}
                        </div>
                      </div>
                      <style>
                        {%- if block.settings.box_style == 'bg-hover' -%}
                        #infobox-{{block.id}}:hover {
                          background-color: {{block.settings.background_color_hover}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_type == 'text' and block.settings.icon_text_color != blank -%}
                        #infobox-{{block.id}} .with-text {
                          color: {{block.settings.icon_text_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-border' and block.settings.icon_border_color != blank -%}
                        #infobox-{{block.id}} .info-box-icon {
                          border-color: {{block.settings.icon_border_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-border' and block.settings.icon_border_hover_color != blank -%}
                        #infobox-{{block.id}}:hover .info-box-icon {
                          border-color: {{block.settings.icon_border_hover_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-bg' and block.settings.icon_background_color != blank -%}
                        #infobox-{{block.id}} .info-box-icon {
                          background-color: {{block.settings.icon_background_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-bg' and block.settings.icon_background_hover_color != blank -%}
                        #infobox-{{block.id}}:hover .info-box-icon {
                          background-color: {{block.settings.icon_background_hover_color}} !important;
                        }
                        {%- endif -%}
                        .vc_custom_{{block.id}}5 {
                          {%- if block.settings.box_background_color != blank -%}
                          background-color: {{block.settings.box_background_color}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_background_image != blank -%}
                          background-image: url({{block.settings.box_background_image | img_url: 'master' }}) !important;
                          background-position: {{block.settings.box_background_position | replace: '_', ' ' }} !important;
                          background-repeat: no-repeat !important;
                          background-size: {{block.settings.box_background_size}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_border_integer != blank -%}
                          border-width: {{block.settings.box_border_integer | remove: ' ' | split: '-' | join: ' '}} !important;
                          border-color: {{block.settings.box_border_color}} !important;
                          border-style: {{block.settings.box_border_style}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_margin_top != blank -%}
                          margin-top: {{block.settings.box_margin_top | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_margin_right != blank -%}
                          margin-bottom: {{block.settings.box_margin_right | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_margin_bottom != blank -%}
                          margin-bottom: {{block.settings.box_margin_bottom | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_margin_left != blank -%}
                          margin-bottom: {{block.settings.box_margin_left | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_padding_top != blank -%}
                          padding-top: {{block.settings.box_padding_top | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_padding_right != blank -%}
                          padding-right: {{block.settings.box_padding_right | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_padding_bottom != blank -%}
                          padding-bottom: {{block.settings.box_padding_bottom | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                          {%- if block.settings.box_padding_left != blank -%}
                          padding-left: {{block.settings.box_padding_left | remove: 'px' | append: 'px'}} !important;
                          {%- endif -%}
                        }
                      </style>
                    </div>
                  </div>
                </div>
                <style>
                  .vc_box_custom_{{ block.id }}7 { 
                    {%- if block.settings.background_color != blank -%}
                    background-color: {{block.settings.background_color}} !important;
                    {%- endif -%}
                    {%- if block.settings.background_image != blank -%}
                    background-image: url({{block.settings.background_image | img_url: 'master' }}) !important;
                    background-position: {{block.settings.background_position | replace: '_', ' ' }} !important;
                    background-repeat: no-repeat !important;
                    background-size: cover !important;
                    {%- endif -%}
                    {%- if block.settings.border_integer != blank -%}
                    border-width: {{block.settings.border_integer | remove: ' ' | split: '-' | join: ' '}} !important;
                    border-color: {{block.settings.border_color}} !important;
                    border-style: {{block.settings.border_style}} !important;
                    {%- endif -%}
                    {%- if block.settings.margin_top != blank -%}
                    margin-top: {{block.settings.margin_top | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.margin_bottom != blank -%}
                    margin-bottom: {{block.settings.margin_bottom | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_top != blank -%}
                    padding-top: {{block.settings.padding_top | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_right != blank -%}
                    padding-right: {{block.settings.padding_right | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_bottom != blank -%}
                    padding-bottom: {{block.settings.padding_bottom | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_left != blank -%}
                    padding-left: {{block.settings.padding_left | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                  } 
                </style>
              </div>
              {%- endfor -%}
            </div> 
          {%- endif -%}
        </div> 
    </div>
  </div>
</div>
